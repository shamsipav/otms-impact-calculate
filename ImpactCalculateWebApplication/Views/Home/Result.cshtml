            if (i % 12 == 0)
            {
                <td rowspan="13">@(i/12+1)</td>
            }

            <tr>
                @*/----------------------------------------------GAS--------------------------------------------------/*@
                <td>
                    <label class="form-control" name="[@i].Gas_A" type="text">@Model.Results[i].Gas_A</label>
                </td>
                <td>
                    <label class="form-control" name="[@i].Gas_V_Alpha" type="text">@Model.Results[i].Gas_V_Alpha</label>
                </td>
                <td>
                    <label class="form-control" name="[@i].Gas_V_Waste" type="text">@Model.Results[i].Gas_V_Waste</label>
                </td>
                @*/----------------------------------------------DEVICE-----------------------------------------------/*@
                <td>
                    <label class="form-control" name="[@i].Device_A" type="text">@Model.Results[i].Device_A</label>
                </td>
                <td>
                    <label class="form-control" name="[@i].Device_V_Alpha" type="text">@Model.Results[i].Device_V_Alpha</label>
                </td>
                <td>
                    <label class="form-control" name="[@i].Device_V_Waste" type="text">@Model.Results[i].Device_V_Waste</label>
