@model IndexViewModel;
@{
    ViewData["Title"] = "Result";
}

<div class="result-content">
    <div class="result-container container-fluid d-flex flex-wrap">
        <div class="gas-content">
            <h1 class="h3">Расчёт по составу отводимых газов</h1>
            <table id="gas-table" class="table table-result table-bordered table-responsive">
                <thead>
                    <tr>
                        <th class="text-center">См</th>
                        <th class="text-center">α<sup>газ</sup>, доли ед</th>
                        <th class="text-center">Vα<sup>г</sup>, м<sup>3</sup>/кг</th>
                        <th class="text-center">V<sub>отх</sub><sup>г</sup>, м<sup>3</sup>/ч</th>
                    </tr>
                </thead>
                <tbody>
                    @{
                        for (int i = 0; i < @Model.Results.Count; i++)
                        {

                            <tr>
                                @if (i % 12 == 0)
                                {
                                    <td rowspan="12">@(i/12+1)</td>
                                }

                                <td>
                                    <p class="text-center">@Math.Round(Model.Results[i].Gas_A, 3)</p>
                                </td>
                                <td>
                                    <p class="text-center">@Math.Round(Model.Results[i].Gas_V_Alpha, 3)</p>
                                </td>
                                <td>
                                    <p class="text-center">@Math.Round(Model.Results[i].Gas_V_Waste, 3)</p>
                                </td>
                            </tr>

                        }
                    }
                </tbody>
            </table>
        </div>
        <div class="device-content">
            <h1 class="h3">Расчёт по приборам</h1>
            <table id="device-table" class="table table-result table-bordered table-responsive">
                <thead>
                    <tr>
                        <th class="text-center">См</th>
                        <th>α<sup>прибор</sup>, доли ед</th>
                        <th>Vα<sup>пр</sup>, м<sup>3</sup>/кг</th>
                        <th>V<sub>отх</sub><sup>пр</sup>, м<sup>3</sup>/ч</th>
                        <th>L<sub>α</sub>, м<sup>3</sup>/кг</th>
                    </tr>
                </thead>

                <tbody>
                    @{
                        for (int i = 0; i < @Model.Results.Count; i++)
                        {

                            <tr>
                                @if (i % 12 == 0)
                                {
                                    <td rowspan="12">@(i/12+1)</td>
                                }

                                <td>
                                    <p class="text-center">@Math.Round(Model.Results[i].Device_A, 3)</p>
                                </td>
                                <td>
                                    <p class="text-center">@Math.Round(Model.Results[i].Device_V_Alpha, 3)</p>
                                </td>
                                <td>
                                    <p class="text-center">@Math.Round(Model.Results[i].Device_V_Waste, 3)</p>
                                </td>
                                <td>
                                    <p class="text-center">@Math.Round(Model.Results[i].La, 3)</p>
                                </td>
                            </tr>

                        }
                    }
                </tbody>
            </table>
        </div>
        <div class="gas-device-difference-content">
            <h1 class="h3">Расчётная разность</h1>
            <table id="gas-device-difference-table" class="table table-result table-bordered table-responsive">
                <thead>
                    <tr>
                        <th class="text-center">См</th>
                        <th>Разность, м<sup>3</sup>/ч</th>
                    </tr>
                </thead>

                <tbody>
                    @{
                        for (int i = 0; i < @Model.Results.Count; i++)
                        {

                            <tr>
                                @if (i % 12 == 0)
                                {
                                    <td rowspan="12">@(i/12+1)</td>
                                }

                                <td>
                                    <p class="text-center">@Math.Round(Model.Results[i].Waste_Difference, 3)</p>
                                </td>
                            </tr>

                        }
                    }
                </tbody>
            </table>
        </div>
        <div class="heat-content">
            <h1 class="h3">Теплосодержание, кДж/м<sup>3</sup></h1>
            <table id="heat-content-table" class="table table-result table-bordered table-responsive">
                <thead>
                    <tr>
                        <th class="text-center">См</th>
                        <th>qCO</th>
                        <th>qCO2</th>
                        <th>qO2</th>
                        <th>qN2</th>
                        <th>qSum</th>
                    </tr>
                </thead>

                <tbody>
                    @{
                        for (int i = 0; i < @Model.Results.Count; i++)
                        {

                            <tr>
                                @if (i % 12 == 0)
                                {
                                    <td rowspan="12">@(i/12+1)</td>
