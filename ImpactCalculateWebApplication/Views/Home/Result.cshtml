                                <td>
                                    <p class="text-center">@Math.Round(Model.Results[i].qCO, 3)</p>
                                </td>
                                <td>
                                    <p class="text-center">@Math.Round(Model.Results[i].qCO2, 3)</p>
                                </td>
                                <td>
                                    <p class="text-center">@Math.Round(Model.Results[i].qO2, 3)</p>
                                </td>
                                <td>
                                    <p class="text-center">@Math.Round(Model.Results[i].qN2, 3)</p>
                                </td>
                                <td>
                                    <p class="text-center">@Math.Round(Model.Results[i].qSum, 3)</p>
                                </td>
                            </tr>

