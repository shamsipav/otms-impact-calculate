@model IndexViewModel;
@{
    ViewData["Title"] = "Result";
}

<table id="table" class="table table-bordered table-responsive">
    <thead class="sticky-header">
        <tr>
            <th>Смены</th>

            @*/------------------------GAS-----------------------/*@
            <th>a<sup>газ</sup>, доли ед</th>
            <th>Va<sup>г</sup>, м<sup>3</sup>/кг</th>
            <th>V<sub>отх</sub><sup>г</sup>, м<sup>3</sup>/ч</th>

            @*/------------------------DEVICE--------------------/*@
            <th>a<sup>прибор</sup>, доли ед</th>
            <th>Va<sup>пр</sup>, м<sup>3</sup>/кг</th>
            <th>V<sub>отх</sub><sup>пр</sup>, м<sup>3</sup>/ч</th>

            @*/------------------------GAS_DEVICE----------------/*@
            <th>Разность, м<sup>3</sup>/ч/%</th>
            <th>L<sub>0</sub>, м<sup>3</sup>/кг</th>

            @*/------------------------MATERIAL_BALANCE----------/*@
            <th>Габбро, кг/ч</th>
            <th>Известняк, кг/ч</th>
            <th>Марг.изв, кг/ч</th>
            <th>Кокс, кг/ч</th>
            <th>Природный газ, кг/ч</th>
            <th>Воздух, кг/ч</th>
            <th>Суммарный приход, кг/ч</th>

            <th>Расплав, кг/ч</th>
            <th>Отходящие газы, кг/ч</th>
            <th>Пыль, кг/ч</th>
            <th>Суммарный расход, кг/ч</th>

            @*/----------MATERIAL_BALANCE_ON_TON_OF_SMELT--------/*@
            <th>Габбро, кг/т</th>
            <th>Известняк, кг/т</th>
            <th>Марг.изв, кг/т</th>
            <th>Кокс, кг/т</th>
            <th>Природный газ, кг/т</th>
            <th>Воздух, кг/т</th>
            <th>Суммарный приход, кг/т</th>

            <th>Расплав, кг/т</th>
            <th>Отходящие газы, кг/т</th>
            <th>Пыль, кг/т</th>
            <th>Суммарный расход, кг/т</th>

