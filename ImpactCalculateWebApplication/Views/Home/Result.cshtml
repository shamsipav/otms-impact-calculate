                                    }

                                    <td>
                                        <p class="text-center">@Math.Round((Model.Results[i].MaterialBalance_Gabbro * Model.Results[i].MaterialBalance_SumPlus / 100), 3)</p>
                                    </td>
                                    <td>
                                        <p class="text-center">@Math.Round((Model.Results[i].MaterialBalance_Limestone * Model.Results[i].MaterialBalance_SumPlus / 100), 3)</p>
                                    </td>
                                    <td>
                                        <p class="text-center">@Math.Round((Model.Results[i].MaterialBalance_M_Limestone * Model.Results[i].MaterialBalance_SumPlus / 100), 3)</p>
                                    </td>
                                    <td>
                                        <p class="text-center">@Math.Round((Model.Results[i].MaterialBalance_Cocks * Model.Results[i].MaterialBalance_SumPlus / 100), 3)</p>
                                    </td>
                                    <td>
                                        <p class="text-center">@Math.Round((Model.Results[i].MaterialBalance_Gas * Model.Results[i].MaterialBalance_SumPlus / 100), 3)</p>
                                    </td>
                                    <td>
                                        <p class="text-center">@Math.Round(Model.Results[i].MaterialBalance_Air, 3)</p>
                                    </td>
                                    <td>
                                        <p class="text-center">@Math.Round(Model.Results[i].MaterialBalance_SumPlus, 3)</p>
                                    </td>
                                </tr>

                            }
                        }
                    </tbody>
                </table>
            </div>
            <div class="material-balance-consumption-content">
