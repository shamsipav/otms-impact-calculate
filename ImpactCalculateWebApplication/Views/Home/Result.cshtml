                <table id="oxides-average-table" class="table table-result table-bordered table-responsive" style="height: fit-content;">
                    <thead>
                        <tr>
                            <th>SiO<sub>2</sub></th>
                            <th>Al<sub>2</sub>O<sub>3</sub></th>
